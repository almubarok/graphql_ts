{
  "name": "graphql_ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build-dev && npm run start",
    "build": "npm run -s migrate && npm run -s generate && tsc",
    "build-dev": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "migrate": "prisma migrate deploy",
    "generate": "npm run -s generate:prisma && npm run -s generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:migrate-dev": "prisma migrate dev --preview-feature",
    "generate:nexus": "ts-node --transpile-only src/module/nexus/schema"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.0.4",
    "@graphql-tools/schema": "^9.0.5",
    "@prisma/client": "^2.23.0",
    "apollo-server-core": "^3.10.3",
    "apollo-server-fastify": "^3.10.3",
    "bcrypt": "^5.1.0",
    "fastify": "^3.29.3",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.11.2",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.3.0",
    "nexus-plugin-prisma": "^0.35.0",
    "ws": "^8.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.7",
    "@types/ws": "^8.5.3",
    "prisma": "^4.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
